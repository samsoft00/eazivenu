<div class="col-item">
    <div class="photo">
        <%= attachment_image_tag(venue.images.first, :thumbnail, :limit, 350, 260, class: "img-responsive") %>
    </div>
    <div class="info">
        <div class="row">
            <div class="price col-md-6">
                <h5><%= venue.name %></h5>
            </div>
            <div class="rating hidden-sm col-md-6">
                <i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star">
                </i><i class="price-text-color fa fa-star"></i><i class="price-text-color fa fa-star">
                </i><i class="fa fa-star"></i>
            </div>
        </div>
        <div class="separator clear-left">
            <p class="btn-add"><%= "&#8358; #{venue.price}".html_safe %></p>
            <p class="btn-details">
                <%= link_to '<i class="fa fa-list"></i> More Details'.html_safe, venue_path(venue), class: "btn btn-sm btn-primary" %>
            </p>
        </div>
        <div class="clearfix">
        </div>
    </div>
</div>